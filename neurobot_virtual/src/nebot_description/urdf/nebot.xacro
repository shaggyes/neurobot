    </inertial>
  </link>




  <joint name="sonar_back_joint" type="fixed">
    <axis xyz="0 1 0" />
    <origin rpy="0 0 3.14" xyz="-0.2 0 0.05" />
    <parent link="chassis"/>
    <child link="back_sonar_forward"/>
  </joint>
